{
    "version": 2,
    "builds": [
        {
            "src": "src/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/src/routes/authRoutes.js"
        },
        {
            "src": "/api/organisations/(.*)",
            "dest": "/src/routes/orgRoutes.js"
        },
        {
            "src": "/api/users/(.*)",
            "dest": "/src/routes/userIdRoutes.js"
        }
    ]
}